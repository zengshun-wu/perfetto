name: sched_get_group_running_task_cnt
ID: 1680
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:int group_id;	offset:8;	size:4;	signed:1;
	field:int win_idx;	offset:12;	size:4;	signed:1;
	field:int cnt;	offset:16;	size:4;	signed:1;

print fmt: "grp_id=%d wc=%d  res=%d", REC->group_id, REC->win_idx, REC->cnt
