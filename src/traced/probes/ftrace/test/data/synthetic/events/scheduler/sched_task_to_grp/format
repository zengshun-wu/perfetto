name: sched_task_to_grp
ID: 1672
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:char comm[16];	offset:8;	size:16;	signed:0;
	field:pid_t pid;	offset:24;	size:4;	signed:1;
	field:int grp_id;	offset:28;	size:4;	signed:1;
	field:int ret;	offset:32;	size:4;	signed:1;
	field:int type;	offset:36;	size:4;	signed:1;

print fmt: "comm=%s pid=%d grp_id=%d ret=%d type=%d", REC->comm, REC->pid, REC->grp_id, REC->ret, REC->type
