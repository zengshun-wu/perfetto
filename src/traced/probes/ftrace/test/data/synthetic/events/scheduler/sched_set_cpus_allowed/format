name: sched_set_cpus_allowed
ID: 1665
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:pid_t pid;	offset:8;	size:4;	signed:1;
	field:unsigned int dest_cpu;	offset:12;	size:4;	signed:0;
	field:bool kthread;	offset:16;	size:1;	signed:0;
	field:unsigned int new_mask;	offset:20;	size:4;	signed:0;
	field:unsigned int valid_mask;	offset:24;	size:4;	signed:0;
	field:unsigned int pause_cpus;	offset:28;	size:4;	signed:0;

