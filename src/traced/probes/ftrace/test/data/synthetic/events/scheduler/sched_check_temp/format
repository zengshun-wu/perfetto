name: sched_check_temp
ID: 1704
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:char unit_name[10];	offset:8;	size:10;	signed:0;
	field:int id;	offset:20;	size:4;	signed:1;
	field:int temp;	offset:24;	size:4;	signed:1;

print fmt: "%s temperature error!! %s=%d temp=%d", REC->unit_name, REC->unit_name, REC->id, REC->temp
